{
  "template": "hsbeat-*",
  "mappings": {
    "_default_": {
      "_source": {
        "enabled": false
      },
      "dynamic_templates": [
        {
          "string_template": {
            "match_mapping_type": "string",
            "mapping": {
              "type": "string",
              "index": "not_analyzed"
            }
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "type": {
          "type": "string",
          "index": "not_analyzed"
        },
        "pid": {
          "type": "integer"
        }
      }
    }
  }
}
