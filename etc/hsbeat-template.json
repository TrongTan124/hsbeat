{
  "template": "hsbeat-*",
  "mappings": {
    "_default_": {
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "pid": {
          "type": "integer"
        },
        "name": {
          "type": "string",
          "index": "not_analyzed"
        },
        "data_type": {
          "type": "byte"
        },
        "long_val": {
          "type": "long"
        },
        "str_val": {
          "type": "string"
        }
      }
    }
  }
}
